<lens name="N4">
    <scene fov-y="90.0" camera-separation-distance="0.062" fbo-width="1024" fbo-height="1024" msaa="1"/>
    <vr-app-settings
        showLoadingIcon = "true"
        useSrgbFramebuffer = "false"
        useProtectedFramebuffer = "false"
        framebufferPixelsWide="-1"
        framebufferPixelsHigh="-1">
        <mode-parms allowPowerSave="true" resetWindowFullScreen="true" cpuLevel="2" gpuLevel="2"/>
        <eye-buffer-parms 
            depthFormat="DEPTH_24"
            textureFilter="TEXTURE_FILTER_ANISO_4"
            colorFormat="COLOR_565"
            multiSamples="2"
            widthScale="1"
            resolution="-1"
            />
        <head-model-parms
            interpupillaryDistance="NaN"
            eyeHeight="NaN"
            headModelDepth="NaN"
            headModelHeight="NaN"
            />        
    </vr-app-settings>
</lens>